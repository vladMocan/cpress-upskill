{
    "name": "cypress-up-skill",
    "version": "1.0.0",
    "description": "A demo project for end to end test using cypress",
    "main": "index.js",
    "scripts": {
        "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",
        "cy:run-safari": "cypress run  --browser webkit  --env allure=true --spec cypress/e2e/FE",
        "cy:run-firefox": "cypress run --browser firefox --env allure=true --spec cypress/e2e/FE",
        "cy:run-chrome": "cypress run --browser chrome --env allure=true  --spec cypress/e2e/FE",
        "cy:run-api": "cypress run --env allure=true --spec cypress/e2e/API ",
        "generate-report": "node scripts/generateReport.js",
        "cy:run-and-report-firefox": "node scripts/runAndGenerateReport.js",
        "cy:run-and-report-chrome": "node scripts/runAndGenerateReport.js",
        "cy:run-and-report-api": "node scripts/runAndGenerateReport.js"
    },
    "author": "Vlad Mocan",
    "license": "ISC",
    "devDependencies": {
        "@shelex/cypress-allure-plugin": "^2.34.0",
        "allure-commandline": "^2.18.1",
        "chai-exclude": "^2.0.2",
        "chai-json-schema": "^1.5.1",
        "cypress": "^10.9",
        "mocha": "^9.1.2",
        "mocha-allure-reporter": "^1.4.0",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^4.2.0",
        "mochawesome-report-generator": "^6.2.0",
        "playwright-webkit": "^1.27.1"
    },
    "dependencies": {
        "fs-extra": "^10.0.0"
    }
}
